<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="300.0" prefWidth="520.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/17" fx:controller="com.ponser2000.appclient.ContractViewController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <TableView fx:id="contractTableView" prefHeight="260.0" prefWidth="500.0">
        <columns>
            <TableColumn fx:id="createDateColumn" prefWidth="120.0" text="Дата Создания"/>
            <TableColumn fx:id="revisionColumn" prefWidth="120.0" text="Номер редакции"/>
            <TableColumn fx:id="updateDateColumn" prefWidth="120.0" text="Дата обновления"/>
            <TableColumn fx:id="actualColumn" prefWidth="120.0" text="Актуальность"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</VBox>
